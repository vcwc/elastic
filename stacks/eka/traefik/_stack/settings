#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export VCW_STACK_GLOBAL_STACKS="\
vcwc/elastic/apm_server/eka,\
vcwc/nginx/rewrite,\
vcwc/nginx/rewrite,\
vcwc/nginx/rewrite"
export VCW_STACK_GLOBAL_STACKNAMES_PREVIOUS="\
vcw_stack_eka,\
vcw_stack_url_rewrite,\
vcw_stack_url_rewrite,\
vcw_stack_url_rewrite"
export VCW_STACK_GLOBAL_STACKNAMES="\
vcw_stack_eka,\
vcw_stack_eka_ext_elasticsearch,\
vcw_stack_eka_ui_apm_server,\
vcw_stack_eka_ui_kibana"
export VCW_STACK_GLOBAL_BOOTSTACKS="\
vcw_stack_eka"
export VCW_STACK_GLOBAL_RUNSTACKS="\
vcw_stack_eka_rest_elasticsearch,\
vcw_stack_eka_ui_apm_server,\
vcw_stack_eka_ui_kibana"
export VCW_STACK_GLOBAL_VOLUME_LIST="\
${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_elasticsearch_volume,\
${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_apm_server_volume,\
${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_nginx_config_volume"
export VCW_STACK_GLOBAL_VOLUME_INITIALISED="\
${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_elasticsearch_volume:/usr/share/elasticsearch/data,\
${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_apm_server_volume:/var/lib/apm_server,\
${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_nginx_config_volume:/vcw/pwd"
# END CONFIGURATION

# START GLOBAL SETTINGS
export VCW_STACK_GLOBAL='\
$VCW_STACK_EKA_EXT_ELASTICSEARCH_ENVIRONMENT_CONF_PATH,\
$VCW_STACK_EKA_EXT_ELASTICSEARCH_ENVIRONMENT_CONF_UPSTREAM,\
$VCW_STACK_EKA_EXT_ELASTICSEARCH_COMPOSE_EXTENSIONS,\
$VCW_STACK_EKA_UI_APM_SERVER_ENVIRONMENT_CONF_PATH,\
$VCW_STACK_EKA_UI_APM_SERVER_ENVIRONMENT_CONF_UPSTREAM,\
$VCW_STACK_EKA_UI_APM_SERVER_COMPOSE_EXTENSIONS,\
$VCW_STACK_EKA_UI_KIBANA_ENVIRONMENT_CONF_PATH,\
$VCW_STACK_EKA_UI_KIBANA_ENVIRONMENT_CONF_UPSTREAM,\
$VCW_STACK_EKA_UI_KIBANA_COMPOSE_EXTENSIONS,\
$VCW_STACK_GLOBAL_NETWORKS,$VCW_STACK_GLOBAL_VOLUMES'
export VCW_STACK_EKA_EXT_ELASTICSEARCH_ENVIRONMENT_CONF_PATH="/${DOLLAR}{VCW_STACK_GLOBAL_URLPREFIX}/elasticsearch"
export VCW_STACK_EKA_EXT_ELASTICSEARCH_ENVIRONMENT_CONF_UPSTREAM="${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_elasticsearch:9200"
export VCW_STACK_EKA_EXT_ELASTICSEARCH_COMPOSE_EXTENSIONS="
      labels: [traefik.enable=true,traefik.docker.network=${DOLLAR}{VCW_STACK_GLOBAL_NETWORKGLOBAL}_overlay,\
      traefik.http.services.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_elasticsearch.loadbalancer.server.port=80,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_elasticsearch.entrypoints=conteco,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_elasticsearch.rule=PathPrefix(\"/${DOLLAR}{VCW_STACK_GLOBAL_URLPREFIX}/elasticsearch\")]
${DOLLAR}{VCW_STACK_EKA_EXT_ELASTICSEARCH_COMPOSE_EXTENSIONS}"
export VCW_STACK_EKA_UI_APM_SERVER_ENVIRONMENT_CONF_PATH="/${DOLLAR}{VCW_STACK_GLOBAL_URLPREFIX}/apm_server"
export VCW_STACK_EKA_UI_APM_SERVER_ENVIRONMENT_CONF_UPSTREAM="${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_apm_server:8200"
export VCW_STACK_EKA_UI_APM_SERVER_COMPOSE_EXTENSIONS="
      labels: [traefik.enable=true,traefik.docker.network=${DOLLAR}{VCW_STACK_GLOBAL_NETWORKGLOBAL}_overlay,\
      traefik.http.services.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_apm_server.loadbalancer.server.port=80,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_apm_server.entrypoints=conteco,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_apm_server.rule=PathPrefix(\"/${DOLLAR}{VCW_STACK_GLOBAL_URLPREFIX}/apm_server\")]
${DOLLAR}{VCW_STACK_EKA_UI_APM_SERVER_COMPOSE_EXTENSIONS}"
export VCW_STACK_EKA_UI_KIBANA_ENVIRONMENT_CONF_PATH="/${DOLLAR}{VCW_STACK_GLOBAL_URLPREFIX}/kibana"
export VCW_STACK_EKA_UI_KIBANA_ENVIRONMENT_CONF_UPSTREAM="${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_kibana:5601"
export VCW_STACK_EKA_UI_KIBANA_COMPOSE_EXTENSIONS="
      labels: [traefik.enable=true,traefik.docker.network=${DOLLAR}{VCW_STACK_GLOBAL_NETWORKGLOBAL}_overlay,\
      traefik.http.services.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_kibana.loadbalancer.server.port=80,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_kibana.entrypoints=conteco,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_GLOBAL_STACKPREFIX}_kibana.rule=PathPrefix(\"/${DOLLAR}{VCW_STACK_GLOBAL_URLPREFIX}/kibana\")]
${DOLLAR}{VCW_STACK_EKA_UI_KIBANA_COMPOSE_EXTENSIONS}"
# END GLOBAL SETTINGS

# START SETTINGS elasticsearch.base
export VCW_STACK_EKA_ELASTICSEARCH_DEPLOY_RESOURCES_LIMITS_MEMORY="756m"
export VCW_STACK_EKA_ELASTICSEARCH_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="512m"
# END SETTINGS elasticsearch.base
# START SETTINGS kibana.base
export VCW_STACK_EKA_KIBANA_DEPLOY_RESOURCES_LIMITS_MEMORY="512m"
export VCW_STACK_EKA_KIBANA_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="256m"
# END SETTINGS kibana.base
# START SETTINGS apm_server
export VCW_STACK_EKA_APM_SERVER_MODULE_APM_SERVER_PORT="8200"
export VCW_STACK_EKA_APM_SERVER_DEPLOY_RESOURCES_LIMITS_MEMORY="512m"
export VCW_STACK_EKA_APM_SERVER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="256m"
# END SETTINGS apm_server
# START SETTINGS nginx.base
export VCW_STACK_EKA_EXT_ELASTICSEARCH_DEPLOY_RESOURCES_LIMITS_MEMORY="64m"
export VCW_STACK_EKA_EXT_ELASTICSEARCH_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="16m"
# END SETTINGS nginx.base
# START SETTINGS nginx.base
export VCW_STACK_EKA_UI_APM_SERVER_DEPLOY_RESOURCES_LIMITS_MEMORY="64m"
export VCW_STACK_EKA_UI_APM_SERVER_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="16m"
# END SETTINGS nginx.base
# START SETTINGS nginx.base
export VCW_STACK_EKA_UI_KIBANA_DEPLOY_RESOURCES_LIMITS_MEMORY="64m"
export VCW_STACK_EKA_UI_KIBANA_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="16m"
# END SETTINGS nginx.base

# START TOPLEVEL SETTINGS
# END TOPLEVEL SETTINGS


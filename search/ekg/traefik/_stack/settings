#!/usr/bin/env bash

# START CONFIGURATION
DOLLAR='$'
export VCW_STACK_GLOBAL_STACKS="\
vcwc/elastic/search/ekg,\
vcwc/nginx/rewrite,\
vcwc/nginx/rewrite,\
vcwc/nginx/rewrite"
export VCW_STACK_GLOBAL_STACKNAMES_PREVIOUS="\
vcw_stack_ekg,\
vcw_stack_url_rewrite,\
vcw_stack_url_rewrite,\
vcw_stack_url_rewrite"
export VCW_STACK_GLOBAL_STACKNAMES="\
vcw_stack_ekg,\
vcw_stack_ekg_rest_elasticsearch,\
vcw_stack_ekg_ui_grafana,\
vcw_stack_ekg_ui_kibana"
export VCW_STACK_GLOBAL_BOOTSTACKS="\
vcw_stack_ekg"
export VCW_STACK_GLOBAL_RUNSTACKS="\
vcw_stack_ekg_rest_elasticsearch,\
vcw_stack_ekg_ui_grafana,\
vcw_stack_ekg_ui_kibana"
export VCW_STACK_GLOBAL_VOLUME_LIST="\
${DOLLAR}{VCW_STACK_STACKPREFIX}_elasticsearch_volume,\
${DOLLAR}{VCW_STACK_STACKPREFIX}_grafana_volume"
export VCW_STACK_GLOBAL_VOLUME_INITIALISED=""
# END CONFIGURATION

# START GLOBAL SETTINGS
export VCW_STACK_GLOBAL='\
$VCW_STACK_EKG_REST_ELASTICSEARCH_ENVIRONMENT_CONF_PATH,\
$VCW_STACK_EKG_REST_ELASTICSEARCH_ENVIRONMENT_CONF_UPSTREAM,\
$VCW_STACK_EKG_REST_ELASTICSEARCH_COMPOSE_EXTENSIONS,\
$VCW_STACK_EKG_UI_GRAFANA_ENVIRONMENT_CONF_PATH,\
$VCW_STACK_EKG_UI_GRAFANA_ENVIRONMENT_CONF_UPSTREAM,\
$VCW_STACK_EKG_UI_GRAFANA_COMPOSE_EXTENSIONS,\
$VCW_STACK_EKG_UI_KIBANA_ENVIRONMENT_CONF_PATH,\
$VCW_STACK_EKG_UI_KIBANA_ENVIRONMENT_CONF_UPSTREAM,\
$VCW_STACK_EKG_UI_KIBANA_COMPOSE_EXTENSIONS,\
$VCW_STACK_GLOBAL_NETWORKS,$VCW_STACK_GLOBAL_VOLUMES'
export VCW_STACK_EKG_REST_ELASTICSEARCH_ENVIRONMENT_CONF_PATH="/${DOLLAR}{VCW_STACK_URLPREFIX}/elasticsearch"
export VCW_STACK_EKG_REST_ELASTICSEARCH_ENVIRONMENT_CONF_UPSTREAM="${DOLLAR}{VCW_STACK_STACKPREFIX}_elasticsearch:9200"
export VCW_STACK_EKG_REST_ELASTICSEARCH_COMPOSE_EXTENSIONS="
      labels: [traefik.enable=true,traefik.docker.network=${DOLLAR}{VCW_STACK_NETWORKGLOBAL}_overlay,\
      traefik.http.services.${DOLLAR}{VCW_STACK_STACKPREFIX}_elasticsearch.loadbalancer.server.port=80,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_STACKPREFIX}_elasticsearch.entrypoints=conteco,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_STACKPREFIX}_elasticsearch.rule=PathPrefix(\"/${DOLLAR}{VCW_STACK_URLPREFIX}/elasticsearch\")]
${DOLLAR}{VCW_STACK_EKG_REST_ELASTICSEARCH_COMPOSE_EXTENSIONS}"
export VCW_STACK_EKG_UI_GRAFANA_ENVIRONMENT_CONF_PATH="/${DOLLAR}{VCW_STACK_URLPREFIX}/grafana"
export VCW_STACK_EKG_UI_GRAFANA_ENVIRONMENT_CONF_UPSTREAM="${DOLLAR}{VCW_STACK_STACKPREFIX}_grafana:3000"
export VCW_STACK_EKG_UI_GRAFANA_COMPOSE_EXTENSIONS="
      labels: [traefik.enable=true,traefik.docker.network=${DOLLAR}{VCW_STACK_NETWORKGLOBAL}_overlay,\
      traefik.http.services.${DOLLAR}{VCW_STACK_STACKPREFIX}_grafana.loadbalancer.server.port=80,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_STACKPREFIX}_grafana.entrypoints=conteco,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_STACKPREFIX}_grafana.rule=PathPrefix(\"/${DOLLAR}{VCW_STACK_URLPREFIX}/grafana\")]
${DOLLAR}{VCW_STACK_EKG_UI_GRAFANA_COMPOSE_EXTENSIONS}"
export VCW_STACK_EKG_UI_KIBANA_ENVIRONMENT_CONF_PATH="/${DOLLAR}{VCW_STACK_URLPREFIX}/kibana"
export VCW_STACK_EKG_UI_KIBANA_ENVIRONMENT_CONF_UPSTREAM="${DOLLAR}{VCW_STACK_STACKPREFIX}_kibana:5601"
export VCW_STACK_EKG_UI_KIBANA_COMPOSE_EXTENSIONS="
      labels: [traefik.enable=true,traefik.docker.network=${DOLLAR}{VCW_STACK_NETWORKGLOBAL}_overlay,\
      traefik.http.services.${DOLLAR}{VCW_STACK_STACKPREFIX}_kibana.loadbalancer.server.port=80,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_STACKPREFIX}_kibana.entrypoints=conteco,\
      traefik.http.routers.${DOLLAR}{VCW_STACK_STACKPREFIX}_kibana.rule=PathPrefix(\"/${DOLLAR}{VCW_STACK_URLPREFIX}/kibana\")]
${DOLLAR}{VCW_STACK_EKG_UI_KIBANA_COMPOSE_EXTENSIONS}"
# END GLOBAL SETTINGS

# START SETTINGS elasticsearch.base
export VCW_STACK_EKG_ELASTICSEARCH_DEPLOY_RESOURCES_LIMITS_MEMORY="756m"
export VCW_STACK_EKG_ELASTICSEARCH_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="512m"
# END SETTINGS elasticsearch.base
# START SETTINGS kibana.base
export VCW_STACK_EKG_KIBANA_DEPLOY_RESOURCES_LIMITS_MEMORY="512m"
export VCW_STACK_EKG_KIBANA_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="256m"
# END SETTINGS kibana.base
# START SETTINGS grafana.base
export VCW_STACK_EKG_GRAFANA_DEPLOY_RESOURCES_LIMITS_MEMORY="128m"
export VCW_STACK_EKG_GRAFANA_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="64m"
# END SETTINGS grafana.base
# START SETTINGS nginx.base
export VCW_STACK_EKG_REST_ELASTICSEARCH_DEPLOY_RESOURCES_LIMITS_MEMORY="64m"
export VCW_STACK_EKG_REST_ELASTICSEARCH_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="16m"
# END SETTINGS nginx.base
# START SETTINGS nginx.base
export VCW_STACK_EKG_UI_GRAFANA_DEPLOY_RESOURCES_LIMITS_MEMORY="64m"
export VCW_STACK_EKG_UI_GRAFANA_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="16m"
# END SETTINGS nginx.base
# START SETTINGS nginx.base
export VCW_STACK_EKG_UI_KIBANA_DEPLOY_RESOURCES_LIMITS_MEMORY="64m"
export VCW_STACK_EKG_UI_KIBANA_DEPLOY_RESOURCES_RESERVATIONS_MEMORY="16m"
# END SETTINGS nginx.base

# START TOPLEVEL SETTINGS
# END TOPLEVEL SETTINGS
